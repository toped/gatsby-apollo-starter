        "@storybook/addon-links": "^6.2.5",
        "@storybook/addons": "^6.2.5",
        "@storybook/react": "^6.2.5",
        "@tailwindcss/postcss7-compat": "^2.1.0",
        "autoprefixer": "^9.8.6",
        "babel-loader": "^8.2.2",
        "concurrently": "^6.0.1",
        "nodemon": "^2.0.7",
        "postcss": "^7.0.35",
        "prettier": "^2.2.1",
        "react-is": "^17.0.2",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.0"
