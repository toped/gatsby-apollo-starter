    "scripts": {
        "dev": "concurrently -k \"nodemon --quiet server\" \"gatsby develop\"",
        "build": "gatsby build",
        "start": "node server",
        "build-start": "npm run-script build && node server",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "lint": "node_modules/eslint/bin/eslint.js src --fix",
        "storybook": "NODE_ENV=production start-storybook -p 6006 -s public",
        "build-storybook": "NODE_ENV=production build-storybook -s public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/toped/gatsby-apollo-starter"
    },
    "bugs": {
        "url": "https://github.com/toped/gatsby-apollo-starter/issues"
    }
