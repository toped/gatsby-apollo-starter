        "countries-list": "^2.6.1",
        "eslint": "^7.23.0",
        "evergreen-ui": "^5.1.2",
        "gatsby": "^3.2.1",
        "gatsby-cli": "^3.2.0",
        "gatsby-image": "^3.2.0",
